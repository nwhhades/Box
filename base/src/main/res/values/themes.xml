<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Aries" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/window_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="colorAccent">@color/focus_pressed</item>
    </style>

    <style name="LoadingProgressBar" parent="Widget.AppCompat.ProgressBar">
        <item name="colorAccent">@color/focus_pressed</item>
        <item name="colorControlActivated">@color/focus_pressed</item>
    </style>

    <style name="TvSeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="colorAccent">@color/focus_focused</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/focus_focused</item>
        <item name="colorControlNormal">@color/white</item>

        <item name="android:thumb">@drawable/abc_seekbar_thumb_material</item>
    </style>

</resources>